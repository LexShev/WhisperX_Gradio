https://github.com/m-bain/whisperX.git

conda create --name whisperx python=3.10
conda activate whisperx
conda install pytorch==2.0.0 torchaudio==2.0.0 numpy==1.26.4 tensorflow==2.10.0 -c pytorch -c nvidia
pip install git+https://github.com/m-bain/whisperx.git
pip install git+https://github.com/m-bain/whisperx.git --upgrade

example
whisperx "E:\TEST_Material\F_Вариант Омега_1975_P02_1080p25_H264_10Mbps.mp4" --device cpu --compute_type int8 --model large-v2 --language ru --output_format srt --output_dir "E:\TEST_Material"